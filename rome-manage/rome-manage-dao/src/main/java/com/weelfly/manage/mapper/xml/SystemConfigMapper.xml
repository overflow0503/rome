<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weelfly.manage.mapper.SystemConfigMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.weelfly.manage.bean.domain.SystemConfig">
		<id column="id" property="id" />
		<result column="config_key" property="configKey" />
		<result column="config_value" property="configValue" />
		<result column="config_description" property="configDescription" />
		<result column="create_user_id" property="createUserId" />
		<result column="update_user_id" property="updateUserId" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
		<result column="remark" property="remark" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
		config_key AS configKey,
		config_value AS configValue,
		config_description AS configDescription,
		create_user_id AS createUserId,
		update_user_id AS updateUserId,
		create_time AS createTime,
		update_time AS updateTime,
		remark
    </sql>

</mapper>
